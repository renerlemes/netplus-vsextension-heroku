{
    "manifestVersion": 1,
    "id": "netplus-vsextension-heroku",
    "publisher": "RenerLemes",
    "version": "1.0.38",
    "name": "Heroku Publish Application",
    "description": "A straightforward utility to publish application in Heroku",
    "public": true,
    "tags": [
        "heroku",
        "git",
        "clone",
        "push",
        "mirror",
        "repo",
        "repository"
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [
        {
            "path": ".release"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "branding": {
        "color": "rgb(255, 255, 255)",
        "theme": "light"
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/renerlemes/netplus-vsextension-heroku.git"
        },
        "support": {
            "uri": "https://github.com/renerlemes/netplus-vsextension-heroku/issues/new"
        },
        "license": {
            "uri": "https://github.com/renerlemes/netplus-vsextension-heroku/blob/main/LICENSE"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/renerlemes/netplus-vsextension-heroku.git"
    },
    "contributions": [
        {
            "id": "netplus-vsextension-heroku",
            "description": "A straightforward utility to publish application in Heroku",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": ".release/heroku-publish-app"
            }
        }
    ]
}